{
	"name": "@edde-js/ssr",
	"version": "5.0.65",
	"description": "Simple SSR support for edde-js",
	"homepage": "https://github.com/edde-js/edde-js#readme",
	"bugs": "https://github.com/edde-js/edde-js/issues",
	"repository": {
		"type": "git",
		"url": "git@github.com:edde-js/edde-js.git"
	},
	"author": "Marek Hanzal",
	"license": "Apache-2.0",
	"devDependencies": {
		"@types/node": "latest",
		"ava": "latest",
		"nyc": "latest",
		"tslint": "latest",
		"typescript": "latest"
	},
	"dependencies": {
		"@types/jsdom": "latest",
		"@types/mime-types": "latest",
		"@types/minimist": "latest",
		"@types/node-cache": "latest",
		"jsdom": "latest",
		"mime-types": "latest",
		"minimist": "latest",
		"node-cache": "latest"
	},
	"main": "src/index",
	"types": "src/index",
	"bin": "./src/ssr.js",
	"files": [
		"src"
	],
	"nyc": {
		"exclude": [
			"src/**/*.spec.js"
		]
	},
	"ava": {
		"files": [
			"src/**/*.spec.js"
		],
		"concurrency": 24,
		"failFast": true,
		"failWithoutAssertions": true
	},
	"scripts": {
		"pretest": "npm run build",
		"test": "node_modules/.bin/nyc node_modules/.bin/ava",
		"build": "find src -type f -name '*.js' -delete && node_modules/.bin/tsc"
	}
}
